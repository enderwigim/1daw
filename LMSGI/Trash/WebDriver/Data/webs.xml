
<webs>
  <pagina>
    <URL>https://wizzair.com/en-gb/newsletter</URL>
    <buttons>
      <button>
        <name>cookie</name>
        <xpath>"//*[@id=\"onetrust-accept-btn-handler\"]"</xpath>
        <type>click</type>
        <insert></insert>
      </button>
      <button>
        <name>name</name>
        <xpath>"//*[@id=\"app\"]/div/main/div/form/fieldset/div[1]/div[1]/div/div/label/input"</xpath>
        <type>text</type>
        <insert>name</insert>
      </button>
      <button>
        <name>surname</name>
        <xpath>"*[@id='app']/div/main/div/form/fieldset/div[1]/div[2]/div/div/label/input"</xpath>
        <type>text</type>
        <insert>surname</insert>
      </button>
      <button>
        <name>gender</name>
        <xpath>"*//*[@id=\"app\"]/div/main/div/form/fieldset/div[1]/div[3]/div/div/label/select"</xpath>
        <type>select</type>
        <insert>0</insert>
      </button>
      <button>
        <name>email</name>
        <xpath>"//*[@id=\"app\"]/div/main/div/form/fieldset/div[2]/div/label/input"</xpath>
        <type>text</type>
        <insert>email</insert>
      </button>
      <button>
        <name>checkbox</name>
        <xpath>"//*[@id=\"app\"]/div/main/div/form/fieldset/div[4]/div[1]/span/label[1]"</xpath>
        <type>button</type>
        <insert></insert>
      </button>
      <button>
        <name>submit</name>
        <xpath>"//*[@id=\"app\"]/div/main/div/form/fieldset/div[4]/div[2]/button"</xpath>
        <type>button</type>
        <insert></insert>
      </button>
    </buttons>
  </pagina>
</webs>
